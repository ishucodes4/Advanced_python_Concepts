import time 

def function1():
    time.sleep(3)
    print("function1")
    
def function2():
    time.sleep(3)
    print("function2")
    
def function3():
    time.sleep(3)
    print("function3")
    
function1()
function2()
function3()

#clearly function1->function2->function3 =>> THIS IS GOING TO BE THE SEQUENCE OF FUNCTION EXECUTION :)def function1():
    
